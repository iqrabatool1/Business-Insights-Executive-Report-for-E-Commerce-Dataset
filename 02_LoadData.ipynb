def load_if_exists(name):
    path = os.path.join(extract_to, name)
    if os.path.exists(path):
        print("Loading", name)
        return pd.read_csv(path)
    else:
        print("Missing:", name)
        return None

orders    = load_if_exists("olist_orders_dataset.csv")
