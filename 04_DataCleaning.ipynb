orders['order_delivered_customer_date'] = pd.to_datetime(orders['order_delivered_customer_date'])
orders['order_delivered_carrier_date'] = pd.to_datetime(orders['order_delivered_carrier_date'])
orders['order_approved_at'] = pd.to_datetime(orders['order_approved_at'])

# Fill missing numeric values with median
for col in ['product_weight_g','product_length_cm','product_height_cm','product_width_cm']:
    products[col].fillna(products[col].median(), inplace=True)

# Fill categorical/text columns
products.fillna({
    'product_category_name': 'unknown',
    'product_name_lenght': 0,
    'product_description_lenght': 0,
    'product_photos_qty': 0
}, inplace=True)

reviews.fillna({
    'review_comment_title': 'No comment',
    'review_comment_message': 'No comment'
}, inplace=True)

# Remove duplicate rows
geo.drop_duplicates(inplace=True)

# Final verification
print(orders.isnull().sum())
print(products.isnull().sum())
print(reviews.isnull().sum())
print("Geo duplicates after cleaning:", geo.duplicated().sum())
